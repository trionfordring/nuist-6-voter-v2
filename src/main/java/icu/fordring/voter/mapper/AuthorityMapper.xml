<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="icu.fordring.voter.mapper.AuthorityMapper">
    <select id="selectAll" resultType="icu.fordring.voter.pojo.Authority">
        SELECT * FROM authority
    </select>

    <select id="selectByName" resultType="icu.fordring.voter.pojo.Authority">
        SELECT * FROM authority WHERE name=#{name}
    </select>
</mapper>